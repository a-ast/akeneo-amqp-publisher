{
    "name": "aa/akeneo-import",
    "description": "Library for product import to Akeneo PIM",
    "type": "library",
    "authors": [
        {
            "name": "Andrey Astakhov",
            "email": "andrey.astakhov@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "aa/akeneo-import-commands": "dev-master",
        "enqueue/amqp-ext": "^0.9",
        "enqueue/dsn": "^0.9.2",
        "symfony/serializer": "^4.2",
        "symfony/property-access": "^4.2",
        "symfony/messenger": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Aa\\AkeneoImport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Aa\\AkeneoImport\\": "spec/"
        }
    },
    "require-dev": {
        "phpspec/phpspec": "^5.1"
    }
}
